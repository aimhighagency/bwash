:@template"6b/* VARS 
-------------------------------------------------------------- */
$page_width: 				100%;
$body_width:				1150px;
$gutter:						40px;
$controls_width:			40px;
$body_pad:					20px;
$main_width: 				680px;
$side_width: 				$body_width - $main_width - $gutter;

$trans:	  					.3s;

/* COLORS  */
$text_main: 				#353535;									/* DARK GREY  */
$text_accent: 				#ddd;
$text_med: 					lighten($text_main, 40%);
$text_lt: 					lighten($text_main, 60%);								/* RED  */
$page_bg:					#fff;
$body_bg:					#f1f1f1;

$font_main:					'Crimson Text';
$font_header:				'Raleway';


/* MIX-INS 
-------------------------------------------------------------- */
@mixin font($font: $font_main, $font_weight: 300){
	font-family: $font, sans-serif;
	font-weight: $font_weight;
}

@mixin link_trans($hov_color: $text_accent){
	cursor: pointer;
	transition: all $trans ease;
	&:hover{all: $hov_color;}
}

.inline_block{
	display: inline-block;
	zoom: 1;
	*display: inline;
}


.clearfix {
    zoom:1;
    &:before, &:after {
        content: "\0020";
        display: block;
        height: 0;
        overflow: hidden;
    }
    &:after {
        clear: both;
    }
}

@mixin bp($point) {
  @if $point == l {
    @media (max-width: 1600px) { @content; }
  }
  @else if $point == m {
    @media (max-width: 768px) { @content; }
  }
  @else if $point == s {
    @media (max-width: 400px)  { @content; }
  }
}




/* COMMON 
-------------------------------------------------------------- */
body{
	@include font;
	background: $page_bg;  
	color: $text_main;
	font-size: 18px;
}

a{
